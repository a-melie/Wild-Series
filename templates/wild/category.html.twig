{% extends 'base.html.twig' %}
{% block body %}
{% for program in programs %}
    <div>
        <h2>{{ program.id }} / {{ program.title }} - Catégorie : {{ program.category.name }}</h2>
        <p>{{ program.summary }}</p>
    </div>
{% else %}
    Aucune série trouvée.
{% endfor %}
<a href="{{ path('app_index') }}" class="text-danger">
    Retour à l'accueil
</a>
{% endblock %}